#ifndef ScavTrap_HPP
#define ScavTrap_HPP



class ScavTrap
{
    public :
        ScavTrap(void);
        ScavTrap(const ScavTrap &cop);
        ScavTrap &operator=(const ScavTrap &eg);
        ~ScavTrap(void);
    private :

};

#endif

#include "ScavTrap.hpp"

ScavTrap::ScavTrap(void){return;}
ScavTrap::ScavTrap(const ScavTrap &cop)
{
    *this = cop;
}
ScavTrap::ScavTrap &operator=(const ScavTrap &eg)
{
    
}
ScavTrap::~ScavTrap(void){return;}


std::string ScavTrap::getName(void)
{
    return this->name;
}

void ScavTrap::setName(std::string str)
{
    this->name = str;
}