#ifndef ClapTrap_HPP
#define ClapTrap_HPP



class ClapTrap
{
    public :
        ClapTrap(void);
        ClapTrap(const ClapTrap &cop);
        ClapTrap &operator=(const ClapTrap &eg);
        ~ClapTrap(void);
    private :

};

#endif

#include "ClapTrap.hpp"

ClapTrap::ClapTrap(void){return;}
ClapTrap::ClapTrap(const ClapTrap &cop)
{
    *this = cop;
}
ClapTrap::ClapTrap &operator=(const ClapTrap &eg)
{
    
}
ClapTrap::~ClapTrap(void){return;}


std::string ClapTrap::getName(void)
{
    return this->name;
}

void ClapTrap::setName(std::string str)
{
    this->name = str;
}